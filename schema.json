{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "collection": {
      "type": "object",
      "properties": {
        "name": { "type": "string" },
        "icon_name": { "type": "string" },
        "icon_color": { "type": "string" },
        "collection_reference_number": { "type": "string", "pattern": "^[0-9]{2,4}$" }
      },
      "required": [
        "name",
        "icon_name",
        "icon_color",
        "collection_reference_number"
      ],
      "additionalProperties": true
    }
  }
}
